{
  "name": "@longmo-utils/node",
  "version": "1.0.0",
  "description": "Node.js-specific utility functions with file system dependencies",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "utils",
    "typescript",
    "nodejs",
    "filesystem"
  ],
  "author": "",
  "license": "MIT"
}
